<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefHeight="338.0" prefWidth="511.0" userData="CreateProject" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProjectCreator.CreateProjectViewController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="54.0" prefWidth="559.0">
         <children>
            <Label text="Title:">
               <font>
                  <Font size="22.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Label>
            <TextField />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="170.0" prefWidth="559.0">
         <children>
            <HBox prefHeight="170.0" prefWidth="341.0">
               <children>
                  <TableView fx:id="RequirementsTable" prefHeight="170.0" prefWidth="343.0">
                    <columns>
                      <TableColumn fx:id="RequrimentsColumn" prefWidth="340.0" text="Requirements" />
                    </columns>
                  </TableView>
               </children>
            </HBox>
            <Button fx:id="Show" mnemonicParsing="false" onAction="#ShowOnClick" text="Show">
               <font>
                  <Font size="16.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="Add" layoutX="371.0" layoutY="15.0" mnemonicParsing="false" onAction="#AddOnClick" text="Add">
               <font>
                  <Font size="16.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="41.0" prefWidth="559.0">
         <children>
            <Label text="Deadline:">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Label>
            <DatePicker fx:id="DateForDeadline">
               <HBox.margin>
                  <Insets top="3.0" />
               </HBox.margin>
            </DatePicker>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="63.0" prefWidth="559.0">
         <children>
            <Button fx:id="Create" mnemonicParsing="false" onAction="#CreateOnClick" text="CREATE">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Button>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#BackOnClick" text="BACK">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets left="345.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
