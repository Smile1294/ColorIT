<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="252.0" prefWidth="505.0" userData="Add Member" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProjectCreator.AddMemberViewController">
   <HBox alignment="CENTER_LEFT" prefHeight="47.0" prefWidth="505.0">
      <Label text="Details:">
         <font>
            <Font size="22.0"/>
         </font>
         <HBox.margin>
            <Insets left="15.0"/>
         </HBox.margin>
      </Label>
   </HBox>
   <HBox prefHeight="44.0" prefWidth="505.0">
      <Label text="Name:">
         <font>
            <Font size="22.0"/>
         </font>
         <HBox.margin>
            <Insets left="15.0"/>
         </HBox.margin>
      </Label>
      <TextField prefHeight="25.0" prefWidth="157.0" promptText="Enter Full Name">
         <HBox.margin>
            <Insets top="5.0"/>
         </HBox.margin>
      </TextField>
   </HBox>
   <HBox layoutX="10.0" layoutY="57.0" prefHeight="45.0" prefWidth="505.0">
      <Label text="Gender:">
         <font>
            <Font size="22.0"/>
         </font>
         <HBox.margin>
            <Insets left="5.0"/>
         </HBox.margin>
      </Label>
      <TextField prefHeight="25.0" prefWidth="157.0" promptText="Gender">
         <HBox.margin>
            <Insets top="5.0"/>
         </HBox.margin>
      </TextField>
   </HBox>
   <HBox layoutX="10.0" layoutY="101.0" prefHeight="43.0" prefWidth="505.0">
      <Label text="Birthday:">
         <font>
            <Font size="22.0"/>
         </font>
         <HBox.margin>
            <Insets left="5.0"/>
         </HBox.margin>
      </Label>
      <TextField promptText="Birthday">
         <HBox.margin>
            <Insets top="5.0"/>
         </HBox.margin>
      </TextField>
   </HBox>
   <HBox layoutX="10.0" layoutY="146.0" prefHeight="45.0" prefWidth="505.0">
      <Label text="Email:">
         <font>
            <Font size="22.0"/>
         </font>
         <HBox.margin>
            <Insets left="5.0"/>
         </HBox.margin>
      </Label>
      <TextField promptText="Email">
         <HBox.margin>
            <Insets top="5.0"/>
         </HBox.margin>
      </TextField>
   </HBox>
   <HBox alignment="CENTER_LEFT" prefHeight="68.0" prefWidth="505.0">
      <Button fx:id="Save" mnemonicParsing="false" onAction="#SaveOnClick" text="Save">
         <font>
            <Font size="18.0"/>
         </font>
         <HBox.margin>
            <Insets left="10.0"/>
         </HBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#BackOnClick" text="Back">
         <HBox.margin>
            <Insets left="365.0"/>
         </HBox.margin>
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
   </HBox>
</VBox>
