<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox prefHeight="331.0" prefWidth="499.0" userData="Project" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Welcome.WelcomeViewController">
   <children>
      <HBox alignment="CENTER" prefHeight="82.0" prefWidth="499.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="73.0" prefWidth="198.0" text="Select a role" textAlignment="CENTER">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="Developer" mnemonicParsing="false" onAction="#DeveloperOnClick" prefHeight="51.0" prefWidth="181.0" text="Developer">
               <font>
                  <Font size="24.0" />
               </font>
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="ProjectCreator" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#ProjectCreatorOnPress" prefHeight="51.0" prefWidth="193.0" text="ProjectCreator">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="10.0" layoutY="110.0" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="ProjectOwner" mnemonicParsing="false" onAction="#ProjectOwnerOnCLick" prefHeight="51.0" prefWidth="189.0" text="ProjectOwner">
               <font>
                  <Font size="24.0" />
               </font>
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="ScrumMaster" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#ScrumMasterOnClick" prefHeight="51.0" prefWidth="193.0" text="Scrum Master">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" prefHeight="64.0" prefWidth="499.0">
         <children>
            <Button mnemonicParsing="false" onAction="#QuitOnClick" prefHeight="39.0" prefWidth="84.0" text="QUIT">
               <font>
                  <Font size="22.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
