<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER_RIGHT" prefHeight="317.0" prefWidth="473.0" userData="Task Table"
      xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="view.Developer.DeveloperTaskTableViewController">
    <HBox prefHeight="260.0" prefWidth="473.0">
        <TableView fx:id="TaskTable" prefHeight="260.0" prefWidth="475.0">
            <columns>
                <TableColumn fx:id="TaskColumn" prefWidth="146.0" text="Task"/>
                <TableColumn fx:id="MembersColumn" prefWidth="174.0" text="Members"/>
                <TableColumn fx:id="HoursWorkedTable" prefWidth="152.0" text="Hours worked"/>
            </columns>
        </TableView>
    </HBox>
    <HBox alignment="CENTER_RIGHT" prefHeight="49.0" prefWidth="473.0">
        <Button mnemonicParsing="false" onAction="#BackOnClick" prefHeight="37.0" prefWidth="71.0" text="BACK">
            <font>
                <Font size="16.0"/>
            </font>
            <HBox.margin>
                <Insets right="10.0"/>
            </HBox.margin>
        </Button>
    </HBox>
</VBox>