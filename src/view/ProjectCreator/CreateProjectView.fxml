<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="165.0" prefWidth="514.0" userData="CreateProject" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ProjectCreator.CreateProjectViewController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="54.0" prefWidth="559.0">
         <children>
            <Label text="Title:">
               <font>
                  <Font size="22.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="Title" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="41.0" prefWidth="559.0">
         <children>
            <Label text="Deadline:">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Label>
            <DatePicker fx:id="DateForDeadline">
               <HBox.margin>
                  <Insets top="3.0" />
               </HBox.margin>
            </DatePicker>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="63.0" prefWidth="347.0">
         <children>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#BackOnClick" prefHeight="39.0" prefWidth="69.0" text="BACK">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="Create" mnemonicParsing="false" onAction="#CreateOnClick" text="CREATE">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets left="330.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
