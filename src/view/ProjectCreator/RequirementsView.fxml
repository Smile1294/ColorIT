<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="377.0" prefWidth="470.0" userData="Requirements" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ProjectCreator.RequirementsViewController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="44.0" prefWidth="459.0">
         <children>
            <Label text="ID:">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="ID" prefHeight="25.0" prefWidth="170.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="44.0" prefWidth="459.0">
         <children>
            <Label text="Who:">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="Who" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="54.0" prefHeight="44.0" prefWidth="459.0">
         <children>
            <Label text="What:">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="What" prefHeight="25.0" prefWidth="143.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="98.0" prefHeight="44.0" prefWidth="459.0">
         <children>
            <Label text="Why:">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="Whz" prefHeight="25.0" prefWidth="143.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="142.0" prefHeight="44.0" prefWidth="459.0">
         <children>
            <Label text="Time Estimate:">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="hours" prefHeight="25.0" prefWidth="143.0" promptText="In Hours" />
         </children>
      </HBox>
      <HBox fx:id="deadline" alignment="CENTER_LEFT" layoutX="10.0" layoutY="186.0" prefHeight="44.0" prefWidth="459.0">
         <children>
            <Label text="Deadline:">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <DatePicker fx:id="Deadline" onAction="#DeadlineOnClck" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="186.0" prefHeight="44.0" prefWidth="459.0">
         <children>
            <Label text="Responsible team member">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <TextField prefHeight="25.0" prefWidth="143.0" promptText="Role" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="274.0" prefHeight="61.0" prefWidth="459.0">
         <children>
            <Button fx:id="Apply" mnemonicParsing="false" onAction="#ApplyOnClick" text="APPLY">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <Button layoutX="20.0" layoutY="21.0" mnemonicParsing="false" onAction="#BackOnClick" text="BACK">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets left="300.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
