<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="324.0" prefWidth="411.0" userData="Requiremnts" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="Developer.DeveloperRequirementsViewController">
    <HBox prefHeight="253.0" prefWidth="486.0">
        <VBox prefHeight="100.0" prefWidth="242.0">
            <TableView fx:id="RequirementListTable" prefHeight="258.0" prefWidth="242.0">
                <columns>
                    <TableColumn fx:id="RequirementListColumn" prefWidth="241.0" text="Requirement list"/>
                </columns>
            </TableView>
        </VBox>
        <VBox prefHeight="253.0" prefWidth="162.0">
            <TextField fx:id="Search" promptText="Search Requirement">
                <VBox.margin>
                    <Insets/>
                </VBox.margin>
            </TextField>
        </VBox>
    </HBox>
    <HBox alignment="BOTTOM_RIGHT" prefHeight="73.0" prefWidth="411.0">
        <Button mnemonicParsing="false" onAction="#BackOnClick" text="BACK">
            <font>
                <Font size="18.0"/>
            </font>
            <HBox.margin>
                <Insets bottom="15.0" right="15.0"/>
            </HBox.margin>
        </Button>
    </HBox>
</VBox>